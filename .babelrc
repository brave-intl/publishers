{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    [
      "react"
    ]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "babel-plugin-styled-components",
    "transform-object-rest-spread",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}

